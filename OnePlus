
class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
     
        n=len(digits)
        x=0
        for i in range(n):
            
            x+=digits[i]*(10**(n-1-i))
            
        y=x+1
        p=[]
        while y>0:
            p.insert(0,y%10)
            y//=10
        
        return p 
