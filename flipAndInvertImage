class Solution(object):
    def flipAndInvertImage(self, image):
        """
        :type image: List[List[int]]
        :rtype: List[List[int]]
        """
        for j in range(len(image)):
            arr1=image[j]
            arr1.reverse()
            for i in range (len(arr1)):
                if arr1[i]==0:
                    arr1[i]=1
                elif arr1[i]==1:
                    arr1[i]=0
        return image


        
