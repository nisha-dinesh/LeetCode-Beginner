class Solution(object):
    def searchInsert(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        n=len(nums)
        first=0
        last=n-1
        
        while first<=last:
            mid=(first+last)//2
            if target==nums[mid]:
                return mid
            elif target>nums[mid]:
                first = mid+1
            elif target<nums[mid]:
                last=mid-1
        
        return first

            
        
        
            
            
