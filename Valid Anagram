class Solution(object):
    def isAnagram(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        S=list(s)
        T=list(t)
        nS=len(S)
        nT=len(T)
        if nS!=nT:
            return False
        S.sort()
        T.sort()

        for i in range(nS):
            if S[i] != T[i]:
                return False
        return True
