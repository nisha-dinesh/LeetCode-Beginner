class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        n=len(nums)
        i=0
    
        for i in range(n):
            j=i+1
            while j<n:
                if nums[i]+nums[j]==target:
                    arr=[i,j]
                    return arr
                j+=1
