class Solution(object):
    def majorityElement(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        counts={}
        for num in nums:
            if num in counts:
                counts[num]+=1
            else:
                counts[num] = 1
        majority=nums[0]
        for key in counts:
            if counts[key] > counts[majority]:
                majority = key

        return majority
        
            
            
