class Solution(object):
    def check(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        n= len(nums)
        less_count=0
        for i in range(n-1):
            if nums[i]> nums[i+1]:
                less_count+=1
        if nums[0]<nums[-1]:
            less_count+=1
        if less_count<=1:
            return True
        return False
