class Solution(object):
    def lengthOfLastWord(self, s):
        """
        :type s: str
        :rtype: int
        """
        n= len(s)
        res=0
        for i in range(n-1,-1,-1):
            if s[i]!=" ":
                res+=1
            elif res>0:
                break
        return res
        
